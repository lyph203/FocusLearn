<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LogController</servlet-name>
        <servlet-class>controller.LogController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageLecturerController</servlet-name>
        <servlet-class>controller.ManageLecturerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewDashboardController</servlet-name>
        <servlet-class>controller.ViewDashboardController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyCourseController</servlet-name>
        <servlet-class>controller.VerifyCourseController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PendingCourseController</servlet-name>
        <servlet-class>controller.PendingCourseController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogController</servlet-name>
        <url-pattern>/log</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewDashboardController</servlet-name>
        <url-pattern>/addashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageLecturerController</servlet-name>
        <url-pattern>/manageLecturer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyCourseController</servlet-name>
        <url-pattern>/verify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PendingCourseController</servlet-name>
        <url-pattern>/pendingcourse</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
